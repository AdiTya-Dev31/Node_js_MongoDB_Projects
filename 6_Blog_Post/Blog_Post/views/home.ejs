<%- include('partials/header') %>
<h1 style="text-align: center;">Make your Personalized Blogs</h1>
<p style="text-align: center;"><%= homeStartingContent %></p>
<hr />

<form class="compose" action="/compose" method="get">
  <button type="submit" class="btn btn-lg btn-primary">Want to Post?</button> <a class="btn btn-lg btn-primary" href="/logout">Logout</a>
</form>
<% posts.forEach(function(post){ %>

<h1><%=post.title%></h1>
<p>
  <%=post.description.substring(0, 100) + " ..."%>
  <a href="/posts/<%=post._id%>">Read More</a>
</p>
<% }) %> <%-include('partials/footer') %>
